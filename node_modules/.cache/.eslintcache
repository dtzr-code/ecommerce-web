],"import React, { useEffect, useState } from \"react\";\nimport \"./styles.css\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport {\n  removeFromCart,\n  selectItems,\n  selectTotal,\n} from \"../../slices/cartSlice\";\nimport { selectUser } from \"../../slices/userSlice\";\nimport Signin from \"../Signin/Signin\";\nimport { loadStripe } from \"@stripe/stripe-js\";\nimport axios from \"axios\";\nimport Checkout from \"../../app/Checkout/Checkout\";\nimport { useHistory } from 'react-router'\n\n\n// const stripePromise = loadStripe(`${process.env.STRIPE_PUBLIC_KEY}`);\nconst stripePromise = loadStripe(\n  \"pk_test_51J1YVxL3fRcUBWQKeGVALCUqUUO53Mrrpdw5Dg7d4rMJGPhFJ16XfHAcM0dwsH4GUPC8XzqQcqvFCQ1Oo2CSlMs0007bvipQ9x\"\n);\n\nfunction Cart({ open, setOpen }) {\n\n  const history = useHistory()\n\n  //To display cart items\n  const items = useSelector(selectItems);\n\n  //To display total price\n  const total = useSelector(selectTotal);\n\n  //To remove item from cart\n  const dispatch = useDispatch();\n\n  const removeItem = (id) => {\n    dispatch(removeFromCart({ id }));\n  };\n\n  //For logging out\n  const user = useSelector(selectUser);\n\n  //Showing sign in modal --> in order to checkout\n  const [openModal, setOpenModal] = useState(false);\n\n  const showModal = () => {\n    setOpenModal(!openModal);\n  };\n\n  // const createCheckout = async () => {\n  //   const stripe = await stripePromise;\n  //   const checkoutSession = axios.post(\"/api/create-checkout-session\", {\n  //     items,\n  //     email: user.email,\n  //   });\n  // };\n\n  const toCheckout = () => {\n    history.push('/create-checkout-session')\n  }\n\n  return (\n    <div className=\"cart__container\">\n      <div className=\"cart__wrapper\">\n        <div className=\"cart__header\">\n          <h1>Your Cart</h1>\n          <p className=\"close--icon\" onClick={() => setOpen(!open)}>\n            x\n          </p>\n        </div>\n\n        {/* Cart Items */}\n        <div className=\"item--wrapper\">\n          {items.map((item) => (\n            <div className=\"single__product--wrapper\" key={item.id}>\n              <img className=\"product_img\" src={item.image} alt=\"product_img\" />\n              <div className=\"product__details\">\n                <p className=\"product_name\">{item.name}</p>\n                <p className=\"product_price\"> ${item.price} </p>\n                <div className=\"product__settings\">\n                  <div className=\"product__counter\">\n                    <button className=\"cart__button\">-</button>\n                    <p>1</p>\n                    <button className=\"cart__button\">+</button>\n                  </div>\n                  <p onClick={() => removeItem(item.id)}>REMOVE</p>\n                </div>\n              </div>\n              <h2 className=\"product__price\">${item.price}</h2>\n            </div>\n          ))}\n        </div>\n\n        <div className=\"subtotal\">\n          <p>Sub-total: </p>\n          <p>${total}</p>\n        </div>\n\n        {/* Checkout button */}\n        <button\n          onClick={user ? () => toCheckout(): () => showModal()}\n          className=\"checkout--button\"\n        >\n          {user ? \"Checkout\" : \"Sign In To Checkout\"}\n        </button>\n        {openModal ? (\n          <Signin openModal={openModal} setOpenModal={setOpenModal} />\n        ) : null}\n      </div>\n    </div>\n  );\n}\n\nexport default Cart;\n","/Users/dtzr/Documents/GitHub/ecommerce-web/src/components/Popular/Popular.js",["89"],"import React from 'react';\nimport { useDispatch } from \"react-redux\";\nimport './styles.css';\nimport products from '../../items/products'\nimport { addToCart } from \"../../slices/cartSlice\";\n\nfunction Popular() {\n  \n  const dispatch = useDispatch();\n\n  const addItem = (id, name, price, description, image) => {\n    const product = {\n      id,\n      name,\n      price,\n      description,\n      image\n    };\n\n    //Sending the product as an action to the REDUX store\n    dispatch(addToCart(product))\n  }\n\n  return (\n    <div className =\"popular__container\">\n      <div className=\"items__wrapper\">\n        <div className=\"items--contents\">\n          {products.map((products) => {\n            return(\n            <div className=\"products--container\" key={products.id}>\n              <div className = \"image--wrapper\">\n                <img className=\"protein--image\" src={products.image}></img>\n              </div>\n              <div className = \"description\">\n                <h1 className=\"tagline\"> FOR YOUR WORKOUTS </h1>\n                <h2 className=\"price\"> ${products.price}</h2>\n                <p className=\"product-desc\">{products.description}</p>\n                <a href='/' className=\"learnMore\">Learn More</a>\n                <div className = \"buttons\">\n                    <button type=\"submit\" onClick={() => addItem(products.id, products.name, products.price, products.description, products.image) } className=\"buy\">Buy</button>\n                  <button type = \"submit\" className=\"subscribe\">Suscribe $75/month</button>\n                </div>\n              </div>\n              </div>\n            )\n          })}\n        </div>\n      </div>\n    </div>\n  )\n}\n\nexport default Popular\n","/Users/dtzr/Documents/GitHub/ecommerce-web/src/items/products.js",[],"/Users/dtzr/Documents/GitHub/ecommerce-web/src/slices/cartSlice.js",[],"/Users/dtzr/Documents/GitHub/ecommerce-web/src/app/store.js",["90"],"import {configureStore } from \"@reduxjs/toolkit\";\nimport persistStore from \"redux-persist/es/persistStore\";\nimport rootReducer from './rootReducer'\n\nexport const store = configureStore({\n  reducer: rootReducer\n})\n\nexport const persistor = persistStore(store)\n\n \nexport default { store, persistor };\n\n\n","/Users/dtzr/Documents/GitHub/ecommerce-web/src/components/signin/signin.js",[],"/Users/dtzr/Documents/GitHub/ecommerce-web/src/components/Signin/Signin.js",["91","92"],"import React, { useEffect, useRef, useState } from \"react\";\nimport { Modal } from \"semantic-ui-react\";\nimport { auth } from \"../../firebase\";\nimport \"./styles.css\";\n\nfunction Signin({ openModal, setOpenModal }) {\n  //Signing In\n  const emailRef = useRef(null);\n  const passwordRef = useRef(null);\n\n  const signIn = (e) => {\n    e.preventDefault();\n\n    auth\n      .signInWithEmailAndPassword(\n        emailRef.current.value,\n        passwordRef.current.value\n      )\n      .then(() => {\n        window.location.reload();\n      })\n      .catch((error) => {\n        alert(error.message);\n      });\n  };\n\n  //For Signing up\n  const NewemailRef = useRef(null);\n  const NewpasswordRef = useRef(null);\n\n  const register = (e) => {\n    e.preventDefault();\n    //everytime we click the register button, we want to create an account with the user email and pw\n\n    auth\n      .createUserWithEmailAndPassword(\n        NewemailRef.current.value,\n        NewpasswordRef.current.value\n      )\n      .then()\n      .catch((error) => {\n        alert(error.message);\n      });\n  };\n\n  //control state of signin/signout component\n  const [signin, setSignin] = useState(true);\n\n  return (\n    <Modal\n      onClose={() => setOpenModal(false)}\n      onOpen={() => setOpenModal(true)}\n      open={openModal}\n    >\n      <div\n        class={`container ${!signin ? \"right-panel-active\" : \"\"}`}\n        id=\"container\"\n      >\n        <div class=\"form-container sign-up-container\">\n          <form action=\"#\">\n            <h1 className=\"sign__h1\">Create Account</h1>\n            <span>or use your email for registration</span>\n            <input ref={NewemailRef} type=\"email\" placeholder=\"Email\" />\n            <input\n              ref={NewpasswordRef}\n              type=\"password\"\n              placeholder=\"Password\"\n            />\n            <button className=\"signup__button\" onClick={register}>\n              Sign Up\n            </button>\n          </form>\n        </div>\n        <div class=\"form-container sign-in-container\">\n          <form action=\"#\">\n            <h1 className=\"sign__h1\">Sign in</h1>\n            <input ref={emailRef} type=\"email\" placeholder=\"Email\" />\n            <input ref={passwordRef} type=\"password\" placeholder=\"Password\" />\n            <a href=\"#\">Forgot your password?</a>\n            <button className=\"signin__button\" onClick={signIn}>\n              Sign In\n            </button>\n          </form>\n        </div>\n        <div class=\"overlay-container\">\n          <div class=\"overlay\">\n            <div class=\"overlay-panel overlay-left\">\n              <h1 className=\"sign__h1\">Welcome Back!</h1>\n              <p className=\"sign__p\">\n                To keep connected with us please login with your personal info\n              </p>\n              <button\n                onClick={() => setSignin(true)}\n                class=\"ghost\"\n                className=\"signIn\"\n              >\n                Sign In\n              </button>\n            </div>\n            <div class=\"overlay-panel overlay-right\">\n              <h1 className=\"sign__h1\">Hello, Friend!</h1>\n              <p className=\"sign__p\">\n                Enter your personal details and start journey with us\n              </p>\n              <button\n                onClick={() => setSignin(false)}\n                class=\"ghost\"\n                className=\"signUp\"\n              >\n                Sign Up\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </Modal>\n  );\n}\n\nexport default Signin;\n","/Users/dtzr/Documents/GitHub/ecommerce-web/src/firebase.js",[],"/Users/dtzr/Documents/GitHub/ecommerce-web/src/slices/userSlice.js",[],"/Users/dtzr/Documents/GitHub/ecommerce-web/src/components/Signout/Signout.js",[],"/Users/dtzr/Documents