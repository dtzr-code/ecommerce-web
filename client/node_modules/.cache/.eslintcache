   dispatch(logout());\n      }\n    });\n    //cleaning up\n    //if the component was to unmount, we don't want to duplicate another listerner\n    return unsubscribe;\n  }, [dispatch, auth]);\n\n  return (\n    <div style={{backgroundColor:'black', height: '100vp'}}>\n      <Hero />\n      <Popular />\n      <FactsPage />\n      <Receipes />\n      <Footer/>\n    </div>\n  );\n}\n","/Users/dtzr/Documents/GitHub/ecommerce web/client/src/slices/userSlice.js",[],"/Users/dtzr/Documents/GitHub/ecommerce web/client/src/slices/cartSlice.js",[],"/Users/dtzr/Documents/GitHub/ecommerce web/client/src/components/Popular/Popular.js",["98","99","100"],"import React, { useState } from \"react\";\nimport { useDispatch } from \"react-redux\";\nimport \"./styles.css\";\nimport { addToCart } from \"../../slices/cartSlice\";\nimport { proteins, supplements } from \"../../items/products\";\n\nfunction Popular() {\n  const dispatch = useDispatch();\n\n  const addItem = (id, name, price, description, image) => {\n    const product = {\n      id,\n      name,\n      price,\n      description,\n      image,\n    };\n\n    //Sending the product as an action to the REDUX store\n    dispatch(addToCart(product));\n  };\n\n  return (\n    <div className=\"popular__container\">\n      <h1> MEET THE BEST OF THE BEST </h1>\n      <div className=\"items__wrapper\">\n        <div className=\"items--contents\">\n          {proteins.map((protein) => {\n            return (\n              <div className=\"products--container\" key={protein.id}>\n                <div className=\"image--wrapper\">\n                  <img className=\"protein--image\" src={protein.image}></img>\n                </div>\n                <div className=\"description\">\n                  <h1 className=\"name\"> {protein.name} </h1>\n                  <h2 className=\"price\"> ${protein.price}</h2>\n                  <p className=\"product-desc\">{protein.description}</p>\n                  <a href=\"/\" className=\"learnMore\">\n                    Learn More\n                  </a>\n                  <div className=\"buttons\">\n                    <button\n                      type=\"submit\"\n                      onClick={() =>\n                        addItem(\n                          protein.id,\n                          protein.name,\n                          protein.price,\n                          protein.description,\n                          protein.image\n                        )\n                      }\n                      className=\"buy\"\n                    >\n                      Buy\n                    </button>\n                    <button type=\"submit\" className=\"subscribe\">\n                      Suscribe $75/month\n                    </button>\n                  </div>\n                </div>\n              </div>\n            );\n          })}\n        </div>\n        <h1 className=\"bars__h1\">\n          THE <span>PERFECT</span> COMBINATION\n        </h1>\n        <div className=\"bars--wrapper\">\n          {supplements.map((supplement) => {\n            return (\n              <div className=\"bars--container\" key={supplement.id}>\n                <div className=\"bars__image--wrapper\">\n                  <img className=\"bars--image\" src={supplement.image}></img>\n                </div>\n                <div className=\"bars__description\">\n                  <h2 className=\"bars__name\"> {supplement.name}</h2>\n                  <h2 className=\"bars__price\"> ${supplement.price}</h2>\n                  <p className=\"bars__product-desc\">{supplement.description}</p>\n                </div>\n                <div className=\"bars__buttons\">\n                  <button\n                    type=\"submit\"\n                    onClick={() =>\n                      addItem(\n                        supplement.id,\n                        supplement.name,\n                        supplement.price,\n                        supplement.description,\n                        supplement.image\n                      )\n                    }\n                    className=\"buy\"\n                  >\n                    Buy\n                  </button>\n                </div>\n              </div>\n            );\n          })}\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Popular;\n","/Users/dtzr/Documents/GitHub/ecommerce web/client/src/components/Footer/Footer.js",["101"],"import React from \"react\";\nimport \"./styles.css\";\n\nfunction Footer() {\n  return (\n    <div className=\"footer__wrapper\">\n      <div className=\"footer__details\">\n        <h1>Contact Us</h1>\n        <div className=\"footer__container\">\n          <div className=\"email__container\">\n            <input type=\"email\" placeholder=\"Email\" />\n            <button type=\"submit\">OK</button>\n          </div>\n          <p>\n            Sign up to receive 10% off your first order. By signing up I agree\n            to the <a href=\"#\">Terms & Conditions</a>\n          </p>\n        </div>\n      </div>\n    </div>\n  );\n}\n\nexport default Footer;\n","/Users/dtzr/Documents/GitHub/ecommerce web/client/src/components/Receipes/Receipes.js",["102"],"/Users/dtzr/Documents/GitHub/ecommerce web/client/src/components/FactsPage/FactsPage.js",[],"/Users/dtzr/Documents/GitHub/ecommerce web/client/src/components/Hero/Hero.js",[],"/Users/dtzr/Documents/GitHub/ecommerce web/client/src/items/products.js",[],"/Users/dtzr/Documents/GitHub/ecommerce web/client/src/items/receipes.js",[],"/Users/dtzr/Documents/GitHub/ecommerce web/client/src/items/facts.js",[],"/Users/dtzr/Documents/GitHub/ecommerce web/client/src/components/Signin/Signin.js",["103","104","105","106","107"],"import React, { useEffect, useRef, useState } from \"react\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { Modal } from \"semantic-ui-react\";\nimport { auth } from \"../../firebase\";\nimport { login, selectUser } from \"../../slices/userSlice\";\nimport \"./styles.css\";\n\nfunction Signin({ openModal, setOpenModal }) {\n\n  const dispatch = useDispatch()\n  const user = useSelector(selectUser)\n\n  //Signing In\n  const emailRef = useRef(null);\n  const passwordRef = useRef(null);\n\n  const signIn = (e) => {\n    e.preventDefault();\n\n    auth\n      .signInWithEmailAndPassword(\n        emailRef.current.value,\n        passwordRef.current.value\n      )\n      .then(() => {\n        alert(\"Login successfully!\")\n        window.location.reload();\n      })\n      .catch((error) => {\n        alert(error.message);\n      });\n  };\n\n  //For Signing up\n  const NewemailRef = useRef(null);\n  const NewpasswordRef = useRef(null);\n\n  const register = (e) => {\n\n    e.preventDefault();\n\n    auth\n      .createUserWithEmailAndPassword(\n        NewemailRef.current.value,\n        NewpasswordRef.current.value\n      )\n      .then(alert(\"Sign up successful!\"))\n      .catch((error) => {\n        alert(error.message);\n      });\n  };\n\n\n  //control state of signin/signout component\n  const [signin, setSignin] = useState(true);\n\n  return (\n    <Modal\n      onClose={() => setOpenModal(false)}\n      onOpen={() => setOpenModal(true)}\n      open={openModal}\n    >\n      <div\n        class={`container ${!signin ? \"right-panel-active\" : \"\"}`}\n        id=\"container\"\n      >\n        <div class=\"form-container sign-up-container\">\n          <form action=\"#\">\n            <h1 className=\"sign__h1\">Create Account</h1>\n            <span>or use your email for registration</span>\n            <input ref={NewemailRef} type=\"email\" placeholder=\"Email\" />\n            <input\n              ref={NewpasswordRef}\n              type=\"password\"\n              placeholder=\"Password\"\n            />\n            <button className=\"signup__button\" onClick={register}>\n              Sign Up\n            </button>\n          </form>\n        </div>\n        <div class=\"form-container sign-in-container\">\n          <form action=\"#\">\n            <h1 className=\"sign__h1\">Sign in</h1>\n            <input ref={emailRef} type=\"email\" placeholder=\"Email\" />\n            <input ref={passwordRef} type=\"password\" placeholder=\"Password\" />\n            <a href=\"#\">Forgot your password?</a>\n            <button className=\"signin__button\" onClick={signIn}>\n              Sign In\n            </button>\n          </form>\n        </div>\n        <div class=\"overlay-container\">\n          <div class=\"overlay\">\n            <div class=\"overlay-panel overlay-left\">\n              <h1 className=\"sign__h1\">Welcome Back!</h1>\n              <p className=\"sign__p\">\n                To keep connected with us please login with your personal info\n              </p>\n              <button\n                onClick={() => setSignin(true)}\n                class=\"ghost\"\n                className=\"signIn\"\n              >\n                Sign In\n              </button>\n            </div>\n            <div class=\"overlay-panel overlay-right\">\n              <h1 className=\"sign__h1\">Hello, Friend!</h1>\n              <p className=\"sign__p\">\n                Enter your personal details and start journey with us\n              </p>\n              <button\n                onClick={() => setSignin(false)}\n                class=\"ghost\"\n                className=\"signUp\"\n              >\n                Sign Up\n              </button>\n            </div>\n          </div>\n        </div>\n      </div>\n    </Modal>\n  );\n}\n\nexport default Signin;\n","/Users/dtzr/Documents/GitHub/ecommerce web/client/src/components/Cart/Cart.js",["108"],"import React, { useState } from \"react\";\nimport \"./styles.css\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport {\n  removeFromCart,\n  selectItems,\n  selectTotal,\n  increaseQty,\n  decreaseQty,\n  clearCart,\n} from \"../../slices/cartSlice\";\nimport { selectUser } from \"../../slices/userSlice\";\nimport Signin from \"../Signin/Signin\";\nimport { loadStripe } from \"@stripe/stripe-js\";\n\nconst stripePromise = loadStripe(\n  `${process.env.REACT_APP_STRIPE_PUBLIC_KEY}`\n);\n\nfunction Cart({ open, setOpen }) {\n\n  //For checking user status\n  const user = useSelector(selectUser);\n\n  //To display cart items\n  const items = useSelector(selectItems);\n\n  //To display total price\n  const total = useSelector(selectTotal);\n\n  //To remove item from cart\n  const dispatch = useDispatch();\n\n  const removeItem = (id) => {\n    dispatch(removeFromCart({ id }));\n  };\n\n  //Showing sign in modal --> in order to checkout\n  const [openModal, setOpenModal] = useState(false);\n\n  const showModal = () => {\n    setOpenModal(!openModal);\n  };\n\n  //For adding qty of items\n  const addQty = (id) => {\n    dispatch(increaseQty({ id }));\n  };\n\n  //For decreasing qty of items\n  const minusQty = (id) => {\n    dispatch(decreaseQty({ id }));\n  };\n\n  //creating stripe payment\n  const handleStripe = async (event) => {\n    // Get Stripe.js instance\n    const stripe = await stripePromise;\n\n    // Call your backend to create the Checkout Session\n    const response = await fetch(\"http://localhost:5000/\", {\n      method: \"POST\",\n      headers: {\n        \"Content-Type\": \"application/json\",\n      },\n      body: JSON.stringify({\n        items: items,\n        email: user.email,\n      }),\n    });\n\n    const session = await response.json();\n\n    // When the customer clicks on the button, redirect them to Checkout.\n    const result = await stripe.redirectToCheckout({\n      sessionId: session.id,\n    });\n\n    if (result.error) {\n      alert(result.error.message);\n    }\n  };\n\n  return (\n    <div className=\"cart__container\">\n      <div className=\"cart__wrapper\">\n        <div className=\"cart__header\">\n          <h1>Your Cart</h1>\n          <p className=\"close--icon\" onClick={() => setOpen(!open)}>\n            x\n          </p>\n        </div>\n\n        {/* Cart Items */}\n        <div className=\"item--wrapper\">\n          {items.map((item) => (\n            <div className=\"single__product--wrapper\" key={item.id}>\n              <img className=\"product_img\" src={item.image} alt=\"product_img\" />\n              <div className=\"product__details\">\n                <p className=\"product_name\">{item.name}</p>\n                <p className=\"product_price\"> ${item.price} </p>\n                <div className=\"product__settings\">\n                  <div className=\"product__counter\">\n                    <button\n                      className=\"cart__button\"\n                      onClick={() => minusQty(item.id)}\n                    >\n                      -\n                    </button>\n                    <p>{item.quantity}</p>\n                    <button\n                      className=\"cart__button\"\n                      onClick={() => addQty(item.id)}\n                    >\n                      +\n                    </button>\n                  </div>\n                  <p onClick={() => removeItem(item.id)}>REMOVE</p>\n                </div>\n              </div>\n              <h2 className=\"product__price\">${item.price * item.quantity}</h2>\n            </div>\n          ))}\n        </div>\n\n        <div className=\"subtotal\">\n          <p>Sub-total: </p>\n          <p>${total}</p>\n        </div>\n\n        {/* Checkout button */}\n        <button\n          onClick={user ? handleStripe : () => showModal()}\n          className=\"checkout--button\"\n        >\n          {user ? \"Checkout\" : \"Sign In To Checkout\"}\n        </button>\n\n        {openModal ? (\n          <Signin openModal={openModal} setOpenModal={setOpenModal} />\n        ) : null}\n      </div>\n    </div>\n  );\n}\n\nexport default Cart;\n","/Users/dtzr/Documents/GitHub/ecommerce web/client/src/components/Signout/Signout.js",[],"/Use